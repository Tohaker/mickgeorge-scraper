{
  "name": "mickgeorge-scraper",
  "description": "Web scraper for the BT Customer Portal",
  "version": "1.1.3",
  "author": {
    "name": "Miles Bardon",
    "email": "milesbardon@gmail.com",
    "url": "https://www.github.com/Tohaker"
  },
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/Tohaker/mickgeorge-scraper.git"
  },
  "private": true,
  "homepage": "./",
  "build": {
    "appId": "net.ltcuk.scraper",
    "productName": "Portal Exporter",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "publish": [
      {
        "provider": "github",
        "owner": "Tohaker",
        "repo": "mickgeorge-scraper"
      }
    ],
    "win": {
      "publish": [
        "github"
      ],
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        }
      ]
    },
    "nsis": {
      "license": "./license.txt",
      "deleteAppDataOnUninstall": true
    }
  },
  "dependencies": {
    "@craco/craco": "^5.6.4",
    "@testing-library/react-hooks": "^3.3.0",
    "@types/history": "^4.7.6",
    "@types/jest": "^24.0.0",
    "@types/node": "^12.0.0",
    "@types/puppeteer": "^3.0.0",
    "@types/react": "^16.9.0",
    "@types/react-csv": "^1.1.1",
    "@types/react-dom": "^16.9.0",
    "@types/react-router-dom": "^5.1.5",
    "@types/styled-components": "^5.1.0",
    "bootstrap": "^4.5.0",
    "connect-timeout": "^1.9.0",
    "electron-context-menu": "^2.0.1",
    "electron-log": "^4.2.0",
    "electron-store": "^5.1.1",
    "electron-updater": "^4.3.1",
    "express": "^4.17.1",
    "promise-retry": "^1.1.1",
    "puppeteer-core": "^3.1.0",
    "react": "^16.13.1",
    "react-bootstrap": "^1.0.1",
    "react-csv": "^2.0.3",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.1",
    "styled-components": "^5.1.0",
    "typescript": "~3.7.2"
  },
  "main": "public/electron.js",
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "test": "craco test",
    "eject": "react-scripts eject",
    "electron": "electron .",
    "electron-dev-win": "set ELECTRON_START_URL=http://localhost:3000 && electron .",
    "electron-dev-unix": "ELECTRON_START_URL=http://localhost:3000 && electron .",
    "pack": "electron-builder --dir",
    "predist": "yarn build",
    "dist": "electron-builder --win",
    "postinstall": "electron-builder install-app-deps",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@storybook/addon-actions": "^5.3.19",
    "@storybook/addon-links": "^5.3.19",
    "@storybook/addons": "^5.3.19",
    "@storybook/preset-create-react-app": "^3.0.0",
    "@storybook/react": "^5.3.19",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "electron": "^9.0.0",
    "electron-builder": "^22.6.1",
    "electron-devtools-installer": "^3.0.0",
    "fetch-mock": "^9.10.1",
    "react-test-renderer": "^16.13.1"
  }
}
